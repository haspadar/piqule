<?xml version="1.0"?>
<psalm
        errorLevel="<< config(psalm.error_level)|default(["1"])|join("") >>"
        xmlns="https://getpsalm.org/schema/config"
        findUnusedCode="false"
>
    <projectFiles>
<< config(psalm.project.directories)|default(["../../src"])|format('        <directory name="%s" />')|join("\n") >>
        <ignoreFiles>
<< config(psalm.project.ignore)|default(["../../vendor"])|format('            <directory name="%s" />')|join("\n") >>
        </ignoreFiles>
    </projectFiles>
</psalm>
